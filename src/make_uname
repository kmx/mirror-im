tecmake $1 MF=imlua_process5 USE_LUA52=Yes $2 $3 $4 $5 $6 $7 $8
tecmake $1 MF=imlua_process5 USE_OPENMP=Yes USE_LUA52=Yes $2 $3 $4 $5 $6 $7 $8
tecmake $1 MF=imlua_jp2 USE_LUA52=Yes $2 $3 $4 $5 $6 $7 $8
